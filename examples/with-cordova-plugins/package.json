{
  "private": true,
  "name": "example.admob.plugins",
  "displayName": "AdmobPluginsExample",
  "version": "1.0.0",
  "scripts": {
    "dev-android": "cordova run android"
  },
  "eslintConfig": {
    "extends": [
      "concise-browser"
    ],
    "globals": {
      "admob": true
    }
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-google-analytics": {
        "GMS_VERSION": "17.0.0"
      },
      "cordova-plugin-whitelist": {},
      "cordova-admob-plus": {
        "APP_ID_ANDROID": "ca-app-pub-3940256099942544~3347511713",
        "APP_ID_IOS": "ca-app-pub-3940256099942544~1458002511",
        "PLAY_SERVICES_VERSION": "18.3.0"
      },
      "cordova-plugin-facebook4": {
        "APP_ID": "123456789",
        "APP_NAME": "myApplication",
        "FACEBOOK_HYBRID_APP_EVENTS": "false",
        "FACEBOOK_ANDROID_SDK_VERSION": "5.13.0"
      },
      "cordova-plugin-googleplus": {
        "REVERSED_CLIENT_ID": "myreversedclientid",
        "WEB_APPLICATION_CLIENT_ID": "mywebapplicationclientid",
        "PLAY_SERVICES_VERSION": "17.0.0"
      },
      "cordova-android-support-gradle-release": {
        "ANDROID_SUPPORT_VERSION": "27.0.2"
      }
    },
    "platforms": [
      "android"
    ]
  },
  "dependencies": {
    "cordova-admob-plus": "^0.32.0",
    "cordova-android": "8.1.0",
    "cordova-android-support-gradle-release": "3.0.1",
    "cordova-plugin-facebook4": "^6.2.0",
    "cordova-plugin-google-analytics": "^1.8.6",
    "cordova-plugin-googleplus": "^8.2.1",
    "cordova-plugin-whitelist": "^1.3.4"
  }
}
