{
  "command": {
    "publish": {
      "private": false,
      "conventionalCommits": true,
      "message": "chore(release): publish"
    }
  },
  "ignoreChanges": [
    "examples/**",
    "internal/**",
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md",
    "**/package.json"
  ],
  "npmClient": "pnpm",
  "version": "independent",
  "$schema": "node_modules/@lerna-lite/cli/schemas/lerna-schema.json"
}
