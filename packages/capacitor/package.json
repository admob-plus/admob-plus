{
  "name": "@admob-plus/capacitor",
  "version": "0.2.2",
  "description": "Capacitor AdMob Plugin",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "run-s clean && tsc -b && test -e dist/esm/index.js",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "run-s build"
  },
  "author": "Ratson",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "@capacitor/ios": "latest",
    "@capacitor/android": "latest",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.4.5"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "AdmobPlusCapacitor.podspec",
    "!**/.DS_Store",
    "!**/.idea/",
    "!**/*.iml",
    "!android/captures",
    "!android/build",
    "!android/.gradle",
    "!android/local.properties",
    "!android/.externalNativeBuild"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "admob"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": "admob-plus/admob-plus",
  "homepage": "https://admob-plus.github.io",
  "publishConfig": {
    "access": "public"
  }
}
