{
  "name": "admob-plus-cordova",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cordova-plus plugin build --input=./src/www/admob.ts --lib --esm",
        "cwd": "packages/cordova"
      },
      "outputs": [
        "{workspaceRoot}/esm",
        "{workspaceRoot}/lib",
        "{workspaceRoot}/www"
      ]
    }
  }
}
